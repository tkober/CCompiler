%{
	
#import "CCParser.h"

%}
%option noyywrap


%%


"+"     { return ADD; }
"-"     { return SUB; }
"*"     { return MUL; }
"/"     { return DIV; }
"|"     { return ABSOLUTE; }
"("     { return LEFT_BRACE; }
")"     { return RIGHT_BRACE; }
[0-9]+  { 
    yylval = atoi(yytext); 
    return NUMBER;
}
\n      { return EOL; }
[ \t]   {}
.       { printf("Unkown character %c\n", *yytext); }


%%


